{
    "configurations": [
        {
            "name": "Linux (Debug)",
            "includePath": [
				"${workspaceFolder}/engine/include",
				"${workspaceFolder}/glm",
				"${workspaceFolder}/nanogui/ext/glad/include",
				"${workspaceFolder}/nanogui/ext/eigen",
				"${workspaceFolder}/nanogui/include",
				"${workspaceFolder}/nanogui/ext/nanovg/src",
				"${workspaceFolder}/gli"
            ],
            "defines": ["_DEBUG",
				"NANOGUI_USE_OPENGL",
				"NANOGUI_GLAD",
				"NANOGUI_SHARED",
				"GLAD_GLAPI_EXPORT"],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "${default}",
            "configurationProvider": "none"
        }
    ],
    "version": 4
}